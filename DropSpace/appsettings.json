{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
"Kestrel": {
  "Endpoints": {
    "Https": {
      "Url": "https://0.0.0.0:8080",
      "Protocols": "Http1AndHttp2",
      "Certificate": {
        "Path": "/etc/letsencrypt/live/dropspace.ru/fullchain.pem",
        "KeyPath": "/etc/letsencrypt/live/dropspace.ru/privkey.pem"
      }
    }
  }
},
  "ConnectionStrings": {
    "Debug": "Host=db;Port=5432;Database=dropspace;Username=postgres;Password=admin;IncludeErrorDetail=true;"
  },
  "OneTimeSecsDuration": 3600,
  "OneTimeRoleName": "OneTimeUser",
  "ChunkSize": 20000000,
  "FilesDirPath": "/temp",
  "SessionDefaultName": "Новая сессия",
  "UploadTimeOutSecs": 30,
  "AllowedHosts": "*",
  "ClientAddress": "http://localhost:7297",
  "KeysFolder": "/keys",
  "RSAFileName": "RSAKey",
  "LoginUserRole": "PermanentUser",
  "PermanentUserSecsDuration": 259200
}
